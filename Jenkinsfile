pipeline{
    agent any
    stages {
        stage('build') {
            steps {
                sh 'echo "creating build"'
            }
        }
        stage('Unit Test') {
            steps {
                sh 'echo "testing"'
            }
        }
        stage('Deploying') {
            steps {
                sh 'echo "Deploying"'
            }
        }
    }
}
